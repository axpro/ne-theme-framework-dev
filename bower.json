{
  "name": "nexteuropa-theme-framework",
  "description": "NextEuropa Theme Framework",
  "main": [
    "src/scripts/bootstrap.js",
    "src/styles/theme.scss"
  ],
  "license": "EUPL-1.1",
  "private": true,
  "dependencies": {
    "jquery": "~2.1.4",
    "bootstrap-sass": "~3.3.5",
    "modernizr": "~3.2.0"
  },
  "devDependencies": {
    "mocha": "~2.3.3",
    "chai": "~3.4.0"
  },
  "overrides": {
    "bootstrap-sass": {
      "main": [
        "assets/stylesheets/**/*",
        "assets/fonts/bootstrap/*",
        "assets/javascripts/bootstrap/**.js"
      ]
    }
  },
  "ignore": [
    "**/.*",
    "docs",
    "test",
    "tools"
  ]
}
